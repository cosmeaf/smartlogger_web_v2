{
  "name": "react_smartlogger",
  "private": true,
  "version": "3.3.4",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "bash scripts/dev-frontend.sh",
    "dev:frontend:simple": "vite --mode development",
    "dev:backend": "bash scripts/dev-backend.sh",
    "dev:backend:simple": "cd backend && npm run dev",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "preview": "vite preview --mode production",
    "preview:dev": "vite preview --mode development",
    "check-ports": "bash scripts/check-ports.sh",
    "check-ports:prod": "bash scripts/check-ports-prod.sh",
    "lint": "eslint .",
    "deploy:frontend": "bash ../deploy_smartlogger_frontend_Ngit.sh",
    "deploy:backend": "bash ../deploy_smartlogger_backend_Ngit.sh",
    "deploy:fullstack": "bash ../deploy_smartlogger_fullstack_Ngit.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "axios": "^1.7.7",
    "buffer": "^6.0.3",
    "chart.js": "^4.5.0",
    "crypto-browserify": "^3.12.1",
    "dotenv": "^16.4.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "maplibre-gl": "^5.6.2",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-paginate": "^8.2.0",
    "react-router-dom": "^6.28.0",
    "react-table": "^7.8.0",
    "react-toastify": "^10.0.6",
    "react-wavify": "^1.11.1",
    "recharts": "^3.1.2",
    "stream-browserify": "^3.0.0",
    "sweetalert2": "^11.14.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.2",
